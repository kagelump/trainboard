{
  "name": "trainboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "start": "http-server -c-1 -p 8080",
    "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx,json,md}\""
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "typescript": "^5.9.0",
    "vite": "^7.1.0",
    "prettier": "^3.6.0",
    "vitest": "^4.0.0",
    "gh-pages": "^5.0.0"
  }
}
